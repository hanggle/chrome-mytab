{
  "manifest_version": 3,
  "name": "自定义新标签页",
  "version": "1.0",
  "description": "自定义新标签页，支持多搜索引擎切换和书签栏控制",
  "permissions": [
    "bookmarks",
    "favicon",
    "windows"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 